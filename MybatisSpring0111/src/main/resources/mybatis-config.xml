<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.mycom.mybatis0111.vo.Member" alias="Member" />
		<typeAlias type="com.mycom.mybatis0111.vo.Board" alias="Board" />
		<typeAlias type="com.mycom.mybatis0111.vo.Comments" alias="Comments" />
	</typeAliases>
	
	<!-- 여기에 있던 애들은 root-context.xml에서 이미 처리된 것 !-->
	
	<mappers>
		<mapper resource="mapper/memberMapper.xml" />
		<mapper resource="mapper/boardMapper.xml" />
		<mapper resource="mapper/commentsMapper.xml" />
	</mappers>
	
</configuration>
